<div id="currentTransactionsPartial"></div>

<button onclick="fetch20more()">Tryck</button>

    @section scripts{
        <script>
            var skip = 0;

            function fetch20more() {
                skip = skip + 20;
                $.ajax({
                    url: "/AccountView/GetFrom?startPos=" + skip,
                    success: function(result) {
                        $("#currentTransactionsPartial").append(result);
                    }
                });
            }
        </script>
    }